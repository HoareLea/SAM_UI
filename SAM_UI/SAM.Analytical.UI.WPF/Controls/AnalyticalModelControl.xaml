<UserControl x:Class="SAM.Analytical.UI.WPF.AnalyticalModelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SAM.Analytical.UI.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TreeView  Margin="0,0,0,0" Name="TreeView_Main">
            <TreeViewItem Header="Analytical Model">
                <TreeViewItem Header="Spaces"></TreeViewItem>
                <TreeViewItem Header="Materials"></TreeViewItem>
                <TreeViewItem Header="Internal Conditions"></TreeViewItem>
            </TreeViewItem>
        </TreeView>
    </Grid>
</UserControl>
