<UserControl x:Class="SAM.Analytical.UI.WPF.AnalyticalModelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SAM.Analytical.UI.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="UserControl_Loaded">
    <Grid>
        <TreeView Margin="0,0,0,0" Name="TreeView_Main" ContextMenuOpening="TreeView_Main_ContextMenuOpening">
            <TreeView.ContextMenu>
                <ContextMenu Name ="ContextMenu"></ContextMenu>
            </TreeView.ContextMenu>


        </TreeView>
    </Grid>
</UserControl>
