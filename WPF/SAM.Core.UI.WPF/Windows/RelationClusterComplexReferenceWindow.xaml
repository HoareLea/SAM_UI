<Window x:Class="SAM.Core.UI.WPF.RelationClusterComplexReferenceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SAM.Core.UI.WPF"
        mc:Ignorable="d"
        Title="Reference" Height="450" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
        </Grid>
        <local:RelationClusterComplexReferenceControl x:Name="RelationClusterComplexReferenceControl_Main" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0, 0, 0, 0"/>
        <Button x:Name="Button_Cancel" IsCancel="True" Content="Cancel" Grid.Row="1" Margin="5,5,5,5" Width="70" HorizontalAlignment="Right"></Button>
        <Button x:Name="Button_OK" Content="OK" Grid.Row="1" Margin="5,5,85,5" HorizontalAlignment="Right" Width="70"/>
    </Grid>
</Window>
