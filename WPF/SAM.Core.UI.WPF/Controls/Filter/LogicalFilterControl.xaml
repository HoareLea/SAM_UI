<UserControl x:Class="SAM.Core.UI.WPF.LogicalFilterControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SAM.Core.UI.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="210">
    <Grid>
        <GroupBox HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <GroupBox.Header>
                <ComboBox x:Name="comboBox_FilterLogicalOperator" Width="75" SelectionChanged="comboBox_FilterLogicalOperator_SelectionChanged"/>
            </GroupBox.Header>
            <Grid x:Name="grid_Filters" Margin="15,10,15,15">
                
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
