<Window x:Class="SAM.Analytical.UI.WPF.BatchCreateViewsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SAM.Analytical.UI.WPF"
        mc:Ignorable="d"
        Title="Create Views" Height="550" Width="350" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="40"></RowDefinition>
        </Grid.RowDefinitions>
        <local:BatchCreateViewsControl x:Name="batchCreateViewsControl" Grid.Row="0" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></local:BatchCreateViewsControl>
        <Button x:Name="button_Cancel" IsCancel="True" Content="Cancel" Grid.Row="1" Width="70" Click="button_Cancel_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,9.6,10"></Button>
        <Button x:Name="button_OK" Content="OK" Grid.Row="1" Click="button_OK_Click" Margin="0,11,84.6,10" HorizontalAlignment="Right" Width="70"/>
    </Grid>
</Window>
